{
    "name": "anahkiasen/flatten",
    "description": "A package for the Illuminate framework that flattens pages to plain HTML",
    "license": "MIT",
    "keywords": [
        "cache",
        "performance",
        "laravel"
    ],
    "authors": [
        {
            "name": "Maxime Fabre",
            "email": "ehtnam6@gmail.com"
        }
    ],
    "require": {
        "illuminate/container": "5.0.*|5.1.*|5.2.*",
        "illuminate/support": "5.0.*|5.1.*|5.2.*",
        "illuminate/cache": "5.0.*|5.1.*|5.2.*",
        "illuminate/config": "5.0.*|5.1.*|5.2.*",
        "illuminate/http": "5.0.*|5.1.*|5.2.*",
        "illuminate/contracts": "5.0.*|5.1.*|5.2.*",
        "mrclay/minify": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "^0.9.4",
        "illuminate/view": "5.0.*|5.1.*|5.2.*",
        "fabpot/php-cs-fixer": "^1.10",
        "symfony/var-dumper": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Flatten\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flatten\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1-dev"
        }
    }
}
