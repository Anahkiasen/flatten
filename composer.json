{
  "name": "anahkiasen/flatten",
  "description": "A package for the Illuminate framework that flattens pages to plain HTML",
  "license": "MIT",
  "keywords": [
    "cache",
    "performance",
    "laravel"
  ],
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com"
    }
  ],
  "require": {
    "illuminate/container": "~4",
    "illuminate/support": "~4",
    "illuminate/cache": "~4",
    "illuminate/config": "~4",
    "illuminate/http": "~4",
    "philf/setting": "~1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3.4",
    "mockery/mockery": "~0.9.2",
    "illuminate/view": "~4"
  },
  "autoload": {
    "psr-4": {
      "Flatten\\": "src/Flatten"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flatten\\": "tests"
    }
  }
}
